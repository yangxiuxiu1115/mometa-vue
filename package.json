{
  "name": "mometa-vue",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@7.22.0",
  "scripts": {
    "example": "pnpm -F @mometa-vue/example run dev",
    "editor": "pnpm -F @mometa-vue/editor run dev",
    "dev:plugin": "esno ./scripts/dev-plugin.ts",
    "build:plugin": "pnpm -F @mometa-vue/editor run build:plugin",
    "build:editor": "pnpm -F @mometa-vue/editor run build:editor",
    "build": "pnpm -F @mometa-vue/editor run build",
    "prepare": "husky install",
    "lint": "eslint",
    "lint:fix": "eslint --fix packages",
    "typecheck": "esno ./scripts/typecheck.ts",
    "test": "vitest run",
    "test:cover": "vitest run --coverage",
    "test:dev": "vitest dev"
  },
  "lint-staged": {
    "packages/**/*.{ts,vue}": [
      "eslint --fix",
      "pnpm run typecheck"
    ]
  },
  "keywords": [],
  "author": "yangxiuxiu",
  "license": "ISC",
  "dependencies": {
    "prettier": "^2.8.1",
    "typescript": "^5.0.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "perfect-debounce": "^0.1.3",
    "picocolors": "^1.0.0",
    "tsup": "^6.5.0",
    "vite": "^4.0.0",
    "vitest": "^0.27.3",
    "vue-tsc": "^1.0.11"
  }
}
